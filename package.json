{
  "name": "store-manager",
  "version": "1.0.0",
  "description": "Store Manager is a web application that helps store owners manage sales and product inventory records",
  "main": "app.js",
  "scripts": {
    "start": "npm run migrate && npm run seed && nodemon --watch server --exec babel-node  -- server/app.js",
    "test": "nyc --reporter=html --reporter=text mocha --exit --compilers js:babel-core/register server/combine.test.js",
    "cover": "istanbul cover _mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "seed": " babel-node server/models/seed.js",
    "undo": "node server/models/dropTables.js",
    "migrate": "babel-node server/models/migration.js",
    "migrate2": "node db.js  createproductSalesTable",
    "migrate3": "node db.js  createproductSalesTable"

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/habibaudu/Store-Manager.git"
  },
