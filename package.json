    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.0",
    "jsonwebtoken": "^8.3.0",
    "make-runnable": "^1.3.6",
    "mocha": "^5.2.0",
    "moment": "^2.22.2",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.4",
    "nyc": "^13.1.0",
    "pg": "^7.6.0",
    "uuid": "^3.3.2",
    "supertest": "^3.3.0",
    "mocha-lcov-reporter": "^1.3.0",
    "istanbul": "^0.4.5",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-jsx-a11y": "^6.1.2",
